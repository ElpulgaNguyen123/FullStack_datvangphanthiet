<% extend("../../datvangphanthiet") %>
<!--=================================
breadcrumb -->
<div class="bg-light">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <ol class="breadcrumb mb-0">
          <li class="breadcrumb-item"><a href="index.html"> <i class="fas fa-home"></i> </a></li>
          <li class="breadcrumb-item"> <i class="fas fa-chevron-right"></i> <a href="#">Library</a></li>
          <li class="breadcrumb-item active"> <i class="fas fa-chevron-right"></i> <span> Property grid </span></li>
        </ol>
      </div>
    </div>
  </div>
</div>
<!--=================================
  breadcrumb -->

<!--=================================
  Listing – grid view -->
<section class="space-ptb">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <div class="section-title mb-3 mb-lg-4">
          <h2><span class="text-primary"><%= title %></h2>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-3 mb-5 mb-lg-0">
        <div class="sidebar">
          <div class="widget">
            <div class="widget-title widget-collapse">
              <h6>Tìm kiếm nâng cao</h6>
              <a class="ml-auto" data-toggle="collapse" href="#filter-property" role="button" aria-expanded="false"
                aria-controls="filter-property"> <i class="fas fa-chevron-down"></i> </a>
            </div>
            <div class="collapse show" id="filter-property">
              <form class="mt-3">
                <div class="input-group mb-2 select-border">
                  <select class="form-control basic-select">
                    <option>Tất cả</option>
                    <option>Căn hộ</option>
                    <option>Đất</option>
                    <option>Nhà</option>
                    <option>Mặt bằng</option>
                  </select>
                </div>
                <div class="input-group mb-2 select-border">
                  <select class="form-control basic-select">
                    <option>Cũ nhất</option>
                    <option>Mới nhất</option>
                  </select>
                </div>
                <div class="input-group mb-2">
                  <button class="btn btn-primary btn-block align-items-center" type="submit"><i
                      class="fas fa-filter mr-1"></i><span>Lọc</span></button>
                </div>
              </form>
            </div>
          </div>
          <div class="widget">
            <div class="widget-title widget-collapse">
              <h6>Vị trí BĐS</h6>
              <a class="ml-auto" data-toggle="collapse" href="#type-property" role="button" aria-expanded="false"
                aria-controls="type-property"> <i class="fas fa-chevron-down"></i> </a>
            </div>
            <div class="collapse show" id="type-property">
              <ul class="list-unstyled mb-0 pt-3">
                <li><a href="#">Khu dân dư<span class="ml-auto">(12)</span></a></li>
                <li><a href="#">Thương mại<span class="ml-auto">(45)</span></a></li>
                <li><a href="#">Công nghiệp<span class="ml-auto">(23)</span></a></li>
                <li><a href="#">Căn hộ<span class="ml-auto">(05)</span></a></li>
                <li><a href="#">Đất<span class="ml-auto">(47)</span></a></li>
                <li><a href="#">Đất huyện xã<span class="ml-auto">(32)</span></a></li>
              </ul>
            </div>
          </div>
          <div class="widget">
            <div class="widget-title widget-collapse">
              <h6>Vị trí địa lý</h6>
              <a class="ml-auto" data-toggle="collapse" href="#type-property" role="button" aria-expanded="false"
                aria-controls="type-property"> <i class="fas fa-chevron-down"></i> </a>
            </div>
            <div class="collapse show" id="type-property">
              <ul class="list-unstyled mb-0 pt-3">
                <li><a href="#">Phan thiết<span class="ml-auto">(12)</span></a></li>
                <li><a href="#">Phong Nẫm<span class="ml-auto">(45)</span></a></li>
                <li><a href="#">Hàm Thắng<span class="ml-auto">(23)</span></a></li>
                <li><a href="#">Bắc Phan thiết<span class="ml-auto">(05)</span></a></li>
                <li><a href="#">Nam Phan Thiết<span class="ml-auto">(47)</span></a></li>
                <li><a href="#">Lagi<span class="ml-auto">(32)</span></a></li>
              </ul>
            </div>
          </div>
          <div class="widget">
            <div class="widget-title">
              <h6>BĐS mới nhất</h6>
            </div>
            <div class="recent-list-item">
              <img class="img-fluid" src="/public/datvangphanthiet/images/property/list/01.jpg" alt="">
              <div class="recent-list-item-info">
                <a class="address mb-2" href="property-detail-style-01.html">Awesome family home</a>
                <span class="text-primary">$1,456,233 </span>
              </div>
            </div>
            <div class="recent-list-item">
              <img class="img-fluid" src="/public/datvangphanthiet/images/property/list/02.jpg" alt="">
              <div class="recent-list-item-info">
                <a class="address mb-2" href="property-detail-style-01.html">Contemporary apartment</a>
                <span class="text-primary">$2,496,454 </span>
              </div>
            </div>
            <div class="recent-list-item">
              <img class="img-fluid" src="/public/datvangphanthiet/images/property/list/03.jpg" alt="">
              <div class="recent-list-item-info">
                <a class="address mb-2" href="property-detail-style-01.html">Family home for sale</a>
                <span class="text-primary">$4,662,457 </span>
              </div>
            </div>
            <div class="recent-list-item">
              <img class="img-fluid" src="/public/datvangphanthiet/images/property/list/04.jpg" alt="">
              <div class="recent-list-item-info">
                <a class="address mb-2" href="property-detail-style-01.html">184 lexington avenue</a>
                <span class="text-primary">$2,456,452 </span>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-lg-9">
        <div class="property-filter d-sm-flex">
          <div class="widget">
            <div class="search">
              <i class="fas fa-search"></i>
              <input id="searchInput" type="text" class="form-control" placeholder="Tìm kiếm...">
            </div>
          </div>
        </div>
        <div class="row mt-4" id="products_search_main">
          <% products.forEach((product) => { %>
          <div class="col-sm-6 col-md-6">
            <div class="property-item">
              <div class="property-image bg-overlay-gradient-04">
                <% if(product.image.length > 0){ %>
                <img class="img-fluid" src="/public/uploads/products/<%=JSON.parse(product.image)[0] %>" alt="">
                <% } else{ %>
                <img class="img-fluid" src="https://via.placeholder.com/500x300" alt="">
                <% } %>
                <div class="property-lable">
                  <span class="badge badge-md badge-primary"><%= product.category_name %></span>
                  <span class="badge badge-md badge-info"> <%= product.sku %> </span>
                </div>
                <span class="property-trending" title="trending"><i class="fas fa-bolt"></i></span>
              </div>
              <div class="property-details">
                <div class="property-details-inner">
                  <h5 class="property-title"><a href="property-detail-style-01.html"><%= product.name %></a>
                  </h5>
                  <span class="property-address"><i class="fas fa-map-marker-alt fa-xs"></i><%= product.address %></span>
                  <span class="property-agent-date"><i class="far fa-clock fa-md"></i><%= product.create_at %></span>
                  <div class="property-price"><%= product.price %><span> vnđ</span> </div>
                  <ul class="property-info list-unstyled">
                    <li class="flex-fill property-bed"><i class="fa fa-eye" aria-hidden="true"></i><span><%= product.view %></span></li>
                    <li class="flex-fill property-bath"><i class="fas fa-map-marker-alt fa-xs"></i><span><%= product.address %></span></li>
                    <li class="flex-fill property-m-sqft"><i class="far fa-square"></i><span><%= product.square %></span></li>
                  </ul>
                </div>
                <div class="property-btn">
                  <a class="property-link" href="/san-pham/<%= product.slug %>.<%= product.id %>" title="<%= product.name %>">Xem chi tiết</a>
                </div>
              </div>
            </div>
          </div>
          <%  }) %>
        </div>
        <div class="row mt-4" id="products_search"></div>
        <div class="row">
          <div class="col-12">
            <ul class="pagination mt-3">
              <li class="page-item disabled mr-auto">
                <span class="page-link b-radius-none">Prev</span>
              </li>
              <li class="page-item active" aria-current="page"><span class="page-link">1 </span> <span
                  class="sr-only">(current)</span></li>
              <li class="page-item"><a class="page-link" href="#">2</a></li>
              <li class="page-item"><a class="page-link" href="#">3</a></li>
              <li class="page-item ml-auto">
                <a class="page-link b-radius-none" href="#">Next</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!--=================================
  Listing – grid view -->

<!--=================================
  newsletter -->
<section class="py-5 bg-primary">
  <div class="container">
    <div class="row">
      <div class="col-md-5">
        <h3 class="text-white mb-0">Sign up to our newsletter to get the latest news and offers.</h3>
      </div>
      <div class="col-md-7 mt-3 mt-md-0">
        <div class="newsletter">
          <form>
            <div class="form-group mb-0">
              <input type="email" class="form-control" placeholder="Enter email">
            </div>
            <button type="submit" class="btn btn-dark b-radius-left-none">Get notified</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</section>
<!--=================================
  newsletter -->